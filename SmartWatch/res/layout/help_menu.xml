<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="${relativePackage}.${activityClass}" >

    <Button
        android:id="@+id/Bathroom_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="114dp"
        android:text="Bathroom " />

    <Button
        android:id="@+id/Eat_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Bathroom_btn"
        android:layout_alignRight="@+id/Bathroom_btn"
        android:layout_below="@+id/Bathroom_btn"
        android:layout_marginTop="71dp"
        android:text="   Eat   " />

    <Button
        android:id="@+id/Back_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignLeft="@+id/Eat_btn"
        android:layout_alignRight="@+id/Eat_btn"
        android:layout_below="@+id/Eat_btn"
        android:layout_marginTop="63dp"
        android:text=" Back " />

</RelativeLayout>
